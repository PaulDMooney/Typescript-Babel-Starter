{
  "name": "typescript-babel-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run type-check && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "jest": "jest",
    "nodemon": "nodemon",
    "nodemon:debug": "nodemon --inspect",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "run:js": "node dist/index",
    "start": "npm run nodemon:debug",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --detectOpenHandles --forceExit --watch a",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.0.0",
    "tsconfig-paths-jest": "0.0.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
